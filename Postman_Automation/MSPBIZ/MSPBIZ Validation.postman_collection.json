			"name": "Login",
			"item": [
				{
					"name": "Login To MSBIZ",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "46e8de9f-1f7a-4fa7-91e6-6281109811c8",
								"exec": [
									"var jsonData = pm.response.json();",
									"pm.environment.set(\"MSPBIZ_oauth_token\", jsonData.token);",
									"pm.environment.set(\"wallet_unique_id\", jsonData.walletUniqueId);",
									"pm.environment.set(\"customer_id\", jsonData.customerId);",
									"pm.environment.set(\"mobile_no\", jsonData.merchantInfo.merchantMSPMobile);",
									"",
									"pm.test(\"Login Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Login Response code is 000\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.responseCode).to.eql(\"000\");",
									"});",
									"",
									"pm.test(\"Login Response Description is Success\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.responseDesc).to.eql(\"Success\");",
									"});"
								],
								"type": "text/javascript"
							}
